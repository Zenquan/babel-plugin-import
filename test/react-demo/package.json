  "scripts": {
    "start": "NODE_ENV=development node scripts/start.js",
    "build": "NODE_ENV=production node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "icons": "pwa-asset-generator ./src/assets/images/icons/logo.svg ./public/static/images/icons -i ./public/index.html -f -m ./public/manifest.json -o false -t png -v static/images/icons",
    "format": "prettier --config .prettierrc.js --write **/*.{css,html,js,jsx,json,less,md,scss,ts,tsx,yml}",
    "lint": "yarn lint:eslint && npm run lint:stylelint",
    "lint:eslint": "eslint -c .eslintrc.js **/*.{js,jsx,ts,tsx}",
    "lint:stylelint": "stylelint --config .stylelintrc.js **/*.{css,less,scss}",
    "ssl": "cross-env mkdir -p config/ssl && mkcert -cert-file config/ssl/cert.pem -key-file config/ssl/key.pem localhost",
    "test:ci": "cross-env CI=true node scripts/test.js --env=jsdom",
    "test:coverage": "yarn test -- --coverage",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache"
  },
  "eslintConfig": {},
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@jomsou/clear-console-plugin": "^1.0.0-master.2b2b392"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testRunner": "/Users/zenquan/Workspace/GitHub/clear-console-plugin/test/react-test-demo/node_modules/jest-circus/runner.js",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
